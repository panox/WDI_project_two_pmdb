<div class="row index">
  <h1>Users</h1>
</div>
<div class="row index">
  <div class="medium-8 medium-offset-2 columns">
    <ul class="medium-block-grid-2">
      <% @users.each do |user| %>
        <li>
          <div>
            <%= link_to user.username, user %>
          </div>
          <div>
            <%= image_tag user.picture %>
          </div>
        </li>
      <% end %>
    </ul>
  </div>
</div>
